{
  "name": "chapter-website",
  "version": "0.5.0",
  "private": true,
  "contributors": [
    "Adrian Darian",
    "Dan Tran",
    "Robert Perrine"
  ],
  "scripts": {
    "build": "ngtw build && ng build --prod --base-href=https://ucmercedacm.github.io/Chapter-Website/",
    "clean": "rm -rf node_modules/ yarn.lock package-lock.json",
    "deploy": "yarn build && ngh --branch=gh-pages --no-silent",
    "docker": "ng serve --host 0.0.0.0 --port 4200 & ngtw watch",
    "e2e": "ng e2e",
    "gren:changelog": "gren changelog --generate --override --tags=all --config=.grenrc.js",
    "gren:release": "gren release --override --tags=all --config=.grenrc.js",
    "lint": "ng lint",
    "ng": "ng",
    "prestart": "ngtw build",
    "start": "ng serve & ngtw watch",
    "store": "ng g module --flat false --module root-store/root-store.module.ts",
    "test": "ng test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "Firefox ESR",
      "not op_mini all",
      "not IE 9-11 # For IE 9-11 support, remove 'not'"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "@fortawesome/angular-fontawesome": "0.5",
    "@fortawesome/fontawesome-svg-core": "~1.2.27",
    "@fortawesome/free-brands-svg-icons": "~5.12.1",
    "@fortawesome/free-regular-svg-icons": "~5.12.1",
    "@fortawesome/free-solid-svg-icons": "~5.12.1",
    "@ngrx/effects": "~8.5.2",
    "@ngrx/entity": "~8.5.2",
    "@ngrx/store": "~8.5.2",
    "@ngrx/store-devtools": "~8.5.2",
    "@popperjs/core": "~2.0.4",
    "angular-cli-ghpages": "~0.6.2",
    "github-release-notes": "^0.17.1",
    "jwt-decode": "~2.2.0",
    "rxjs": "~6.5.3",
    "tslib": "~1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.14",
    "@angular/cli": "~8.3.14",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@ngrx/schematics": "~8.5.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.12.14",
    "codelyzer": "~5.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "ng-tailwindcss": "~2.1.1",
    "protractor": "~5.4.0",
    "tailwindcss": "~1.2.0",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "typescript": "~3.5.0"
  }
}
